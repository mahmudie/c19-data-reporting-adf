{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "c19-data-reporting-adf"
		},
		"ls_ablob_c19reportingsa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_c19reportingsa'"
		},
		"ls_adls_c19reportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_c19reportingdl'"
		},
		"ls_adls_c19reportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://c19stroragedl2.dfs.core.windows.net/"
		},
		"ls_https_opendata_ecdc_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseUrl}"
		},
		"tr_ingest_hospital_admission_data_properties_pl_ingests_ecdc_data_parameters_sourceRelativeUrl": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"tr_ingest_hospital_admission_data_properties_pl_ingests_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/7c2b9f6f-40a7-4687-8982-9e0c10a926a9/resourceGroups/C19-rg/providers/Microsoft.Storage/storageAccounts/c19reportingsa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "c19-data-reporting-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_c19reportingsa_connectionString": {
						"value": "[parameters('ls_ablob_c19reportingsa_connectionString')]"
					},
					"ls_adls_c19reportingdl_accountKey": {
						"value": "[parameters('ls_adls_c19reportingdl_accountKey')]"
					},
					"ls_adls_c19reportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_c19reportingdl_properties_typeProperties_url')]"
					},
					"ls_https_opendata_ecdc_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_https_opendata_ecdc_europa_eu_properties_typeProperties_url')]"
					},
					"tr_ingest_hospital_admission_data_properties_pl_ingests_ecdc_data_parameters_sourceRelativeUrl": {
						"value": "[parameters('tr_ingest_hospital_admission_data_properties_pl_ingests_ecdc_data_parameters_sourceRelativeUrl')]"
					},
					"tr_ingest_hospital_admission_data_properties_pl_ingests_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('tr_ingest_hospital_admission_data_properties_pl_ingests_ecdc_data_parameters_sinkFileName')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}